#! /bin/sh

# getsnet: download libsnet source code from SF.net

proj="libsnet"
rev="1.0.0"
gitroot="git://${proj}.git.sourceforge.net/gitroot/${proj}/${proj}"

git clone ${gitroot}							\
	&& (cd "${proj}"						\
	    && git checkout -b "${proj}-${rev}-branch" "${proj}-${rev}"	\
	    && autoconf)

exit $?
